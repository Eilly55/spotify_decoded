<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="?brightAccentBackgroundBase" />
            <corners android:radius="@dimen/chip_button_corner_radius" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="1500.0dip" />
            <solid android:color="?baseBackgroundTintedHighlight" />
        </shape>
    </item>
</selector>