<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="ring" android:useLevel="false" android:thickness="4.0dip">
            <solid android:color="@color/gray_80" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:useLevel="true" android:thickness="4.0dip">
                <solid android:color="@color/green" />
            </shape>
        </rotate>
    </item>
</layer-list>