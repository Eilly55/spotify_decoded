<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/glue_button_primary_white_background_disabled" />
            <size android:height="@dimen/std_32dp" />
            <corners android:radius="@dimen/std_16dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/glue_button_primary_white_background_pressed" />
            <size android:height="@dimen/std_32dp" />
            <corners android:radius="@dimen/std_16dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/glue_button_primary_white_background_default" />
            <size android:height="@dimen/std_32dp" />
            <corners android:radius="@dimen/std_16dp" />
        </shape>
    </item>
</selector>